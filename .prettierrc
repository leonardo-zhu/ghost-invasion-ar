{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": true,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": false,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json",
        "useTabs": false
      }
    },
    {
      "files": "package.json",
      "options": {
        "parser": "json",
        "useTabs": false
      }
    },
    {
      "files": "jest.config.json",
      "options": {
        "parser": "json",
        "useTabs": false
      }
    },
    {
      "files": ".babelrc",
      "options": {
        "parser": "json",
        "useTabs": false
      }
    }
  ]
}
